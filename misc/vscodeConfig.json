{
    "php.executablePath": "/usr/bin/php",
    "window.zoomLevel": 0,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "vue"
      ],

     // A multiplier to be used on the `deltaX` and `deltaY` of mouse wheel scroll events
    "editor.mouseWheelScrollSensitivity": 0.75,

    // "workbench.colorCustomizations": {
    //     "editor.background": "#1a1c1e",
